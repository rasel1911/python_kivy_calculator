<Button>
    size_hint:(0.2,0.2)
    font_size: 60
<MyLayout>
    BoxLayout:
        
        orientation: "vertical"
        size: root.width, root.height
        TextInput:
            id: text_input
            text:"0"
            halign : "right"
            font_size: 56
            size_hint:(1,0.2)
        GridLayout:
            cols:5
            rows:5
            
                
            Button:
                id:clear_id
                text: "mod."
                color: (0,0,0,1)
                background_color:(0,178/255,1,1)
                font_size: 45
                on_press:root.press("%")
            Button:
                text: "("
                on_press:root.press("(")
                color: (0,1,0,1)
                background_color:(0,178/255,1,1)
            Button:
                text: ")"
                color: (0,1,0,1)
                on_press:root.press(")")
                background_color:(0,178/255,1,1)

            Button:
                text: "%"
                on_press:root.pressen()
                color: (0,1,0,1)
                background_color:(0,178/255,1,1)
            Button:
                text: "/"
                on_press:root.press("/")
                color: (0,1,0,1)
                background_color:(0,178/255,1,1)

            
            Button:
                on_press:root.squ()
                background_color:(0,178/255,1,1)
                Image:
                    source: "square-root.png"
                    center_x: self.parent.center_x
                    center_y: self.parent.center_y
                    color:(1,1,1,1)

            Button:
                text: "7"
                on_press : root.press(7)
            Button:
                text: "8"
                on_press : root.press(8)
            Button:
                text: "9"
                on_press : root.press(9)
            Button:
                text: "x"
                on_press:root.press("*")
            
            Button:
                text: "+/-"
                color:(0,0,0,1)
                background_color:(0,178/255,1,1)
                on_press: root.AbChange()

            
            Button:
                text: "4"
                on_press : root.press(4)
            Button:
                text: "5"
                on_press : root.press(5)
            Button:
                text: "6"
                on_press : root.press(6)
            Button:
                text: "-"
                on_press:root.press("-")
            Button:
                text: "C"
                color: (1,0,0,1)
                background_color:(0,178/255,1,1)
                on_press:root.clear_last()
            
            Button:
                text: "1"
                on_press : root.press(1)
            Button:
                text: "2"
                on_press : root.press(2)
            Button:
                text: "3"
                on_press : root.press(3)
            Button:
                text: "+"
                on_press:root.press("+")
            Button:
                id:clear_id
                text: "CE"
                background_color:(0,178/255,1,1)
                color: (1,0,0,1)
                on_press:root.clear()
            Button:
                text: "0"
                on_press : root.press(0)
            Button:
                text: "00"
                on_press : root.press("00")
            Button:
                text: "."
                on_press : root.press(".")
            Button:
                text: "="
                on_press:root.result()
        
        Label:
            text:"calculator_sel"
            size_hint:(1,0.05)
            
            color:(102/255,1,1,1)
            

            
            


